page {
  background: #f5f6fa;
}
.substrate {
  width: 100%;
  height: 364rpx;
  background: linear-gradient(135deg, #6f8fea 0%, #474ad9 100%);
  border-radius: 0px 0px 0px 128rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.substrate .title_wrap {
  margin-top: 30rpx;
}
.substrate .title_wrap text {
  width: 82rpx;
  height: 24px;
  font-size: 17px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 24px;
  letter-spacing: 14rpx;
}
.substrate .search_wrap {
  display: flex;
  margin-top: 30rpx;
  width: 100%;
  flex-wrap: nowrap;
  align-items: center;
}
.substrate .search_wrap .search_input {
  width: 90%;
}
.substrate .search_wrap .icon_wrap {
  align-items: center;
}
