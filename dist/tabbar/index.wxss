@import '../common/index.wxss';

.van-tabbar {
    display: -webkit-flex;
    display: flex;
    width: 100%;
    height: 50px;
    height: var(--tabbar-height, 50px);
    background-color: #fff;
    background-color: var(--tabbar-background-color, #fff);
    border-radius:25px 25px 0px 0px ;
    box-shadow: 4px 16px 48px -8px rgba(111, 143, 234, 0.16), 2px 6px 16px -10px rgba(111, 143, 234, 0.1);
}

.van-tabbar--fixed {
    position: fixed;
    bottom: 0;
    left: 0
}

.van-tabbar--safe {
    height:84px;
    padding-bottom: 34px;
}